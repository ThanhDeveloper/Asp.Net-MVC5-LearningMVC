@model Sec2Exer1.ViewModel.CustomerViewModel
@{
    ViewBag.Title = "RandomMovie";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
@if (Model.MoviesList.Count == 0)
{
    <p>Customers not founds</p>
}
<ul>
    @{
        foreach (var movie in Model.MoviesList)
        {
            <li>@movie.Name</li>
        }
    }
</ul>
